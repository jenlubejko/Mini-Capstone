<h1>Products</h1>
<div class="row">
  <% @product.each do |product| %>
    <div class="col-md-4"
      <div class="well">
        <h3>Name: <%= product.name %></h3>
        <h4>Price: <%= product.price %></h4>
        <img src="<%= product.image %>" style="width:304px;height:228px;">
        <h4><%= product.description %></h4>
    </div>
  </div>
<% end %>
</div>